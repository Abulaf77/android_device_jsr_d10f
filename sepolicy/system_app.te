# Battery capacity reading
allow system_app sysfs_battery_supply:dir search;
allow system_app sysfs_battery_supply:file { read getattr open };

# JSR-Settings.apk
allow system_app storages-config_prop:property_service set;

# Torch
allow system_app torch_sysfs:file { write open getattr };
allow system_app torch_sysfs:dir { search };
