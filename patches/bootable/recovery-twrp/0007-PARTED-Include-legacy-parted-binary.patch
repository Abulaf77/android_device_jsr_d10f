From 0a0d267a04c9036aa7f84d2fd9b33eb5e91f76e4 Mon Sep 17 00:00:00 2001
From: S-trace <S-trace@list.ru>
Date: Sun, 31 Jan 2016 15:24:52 +0300
Subject: [PATCH 7/7] [PARTED] Include legacy parted binary

Change-Id: Ib788fd0c16b764525d995db938e04b847c25d792
---
 Android.mk          |  1 +
 prebuilt/Android.mk | 14 +++++++-------
 2 files changed, 8 insertions(+), 7 deletions(-)

diff --git a/Android.mk b/Android.mk
index 0fb934d..e2bb90b 100644
--- a/Android.mk
+++ b/Android.mk
@@ -384,6 +384,7 @@ ifneq ($(TW_NO_EXFAT), true)
     endif
 endif
 ifeq ($(BOARD_HAS_NO_REAL_SDCARD),)
+    LOCAL_ADDITIONAL_DEPENDENCIES += parted
     ifeq ($(shell test $(PLATFORM_SDK_VERSION) -gt 22; echo $$?),0)
         LOCAL_ADDITIONAL_DEPENDENCIES += sgdisk
     else
diff --git a/prebuilt/Android.mk b/prebuilt/Android.mk
index 949e359..13ecbe1 100644
--- a/prebuilt/Android.mk
+++ b/prebuilt/Android.mk
@@ -266,13 +266,13 @@ ifeq ($(BOARD_HAS_NO_REAL_SDCARD),)
 	    include $(BUILD_PREBUILT)
 	endif
 	#parted
-	#include $(CLEAR_VARS)
-	#LOCAL_MODULE := parted
-	#LOCAL_MODULE_TAGS := eng
-	#LOCAL_MODULE_CLASS := RECOVERY_EXECUTABLES
-	#LOCAL_MODULE_PATH := $(TARGET_RECOVERY_ROOT_OUT)/sbin
-	#LOCAL_SRC_FILES := $(LOCAL_MODULE)
-	#include $(BUILD_PREBUILT)
+	include $(CLEAR_VARS)
+	LOCAL_MODULE := parted
+	LOCAL_MODULE_TAGS := eng
+	LOCAL_MODULE_CLASS := RECOVERY_EXECUTABLES
+	LOCAL_MODULE_PATH := $(TARGET_RECOVERY_ROOT_OUT)/sbin
+	LOCAL_SRC_FILES := $(LOCAL_MODULE)
+	include $(BUILD_PREBUILT)
 endif
 
